<h1>Sign In</h1>

<%= form_tag(users_url, method: 'post') do %>

  <div>
    <%= label_tag :email %><br />
    <%= text_field_tag :email %>
  </div>

  <div>
    <%= label_tag :password %><br />
    <%= text_field_tag :password %>
  </div>

  <div>
    <%= submit_tag "Sign In" %>
  </div>
<% end %>
